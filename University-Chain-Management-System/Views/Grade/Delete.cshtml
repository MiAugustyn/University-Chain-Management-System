@{
}


<form method="post" asp-action="Delete">

    <input type="hidden" asp-for="Id" />

    <div class="form-group">
        <label asp-for="Value"></label>
        <input asp-for="Value" class="form-control" placeholder="Description" readonly>
        <span asp-validation-for="Value" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="StudentId">Student</label>
        <input asp-for="StudentId" class="form-control" placeholder="Description" readonly> 
        <span asp-validation-for="StudentId" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="SubjectId">Subject</label>
        <input asp-for="SubjectId" class="form-control" placeholder="Description" readonly>
        <span asp-validation-for="SubjectId" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
</form>
