@model Grade
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "Grades";
}

<div class="container">

    <h1 class="my-4">Grade: @Model.Value</h1>

    <div class="row">
        <div class="col-md-4">
            <h3 class="my-3">Informations:</h3>

            <ul>
                <li>ID: @Model.Id</li>
                <li>Student: @Model.Student?.FullName</li>
                <li>Subject: @Model.Subject?.Name</li>
            </ul>
        </div>

    </div>

</div>
